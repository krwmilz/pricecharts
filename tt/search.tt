[% WRAPPER wrapper.tt %]
	<p>Found <b>[% num_results %]</b> search
	[% IF num_results == 1 %]
		result
	[% ELSE %]
		results
	[% END %]
	for <b>[% query %]</b>:</p>
	
	[% FOREACH part_num IN results %]
	<div class="product">
		<b>[% part_num.0 %]</b> [% part_num.1 %]<br>

		<object data="/svg/[% part_num.2 %].svg" type="image/svg+xml">
		</object>
	</div>
	[% END %]
[% END %]
