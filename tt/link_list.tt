[% WRAPPER wrapper.tt %]
	<div class="column">
		<h1>[% name %] List ([% num %])</h1>
		<ul>
		[% FOREACH manufacturer IN manufacturers %]
			<li><a href="/[% dir_prefix %]/[% manufacturer.0 %].html">
				<img alt="[% manufacturer.1 %]" class="logo"
					src="/logo/[% manufacturer.0 %].svg">
				</img>
			</a>
		[% END %]
		</ul>
	</div>
[% END %]
