[% WRAPPER wrapper.tt %]
	<h1><img alt="[% name_lc %]" class="logo" src="/logo/[% name_lc %].svg"></img>
		([% num_products %] total)</h1>
	[% FOREACH part_num IN products %]
		<div class="product">
			<img alt="[% part_num.1 %]" class="logo_small" src="/logo/[% part_num.1 %].svg"></img>
			[% part_num.2 %]<br>
			[% part_num.0 %]<br>
			<object data="/svg/[% part_num.0 %].svg" type="image/svg+xml">
			</object>
		</div>
	[% END %]
[% END %]
